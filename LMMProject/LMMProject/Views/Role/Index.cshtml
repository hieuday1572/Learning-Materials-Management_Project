@model IEnumerable<Role>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<table>
    <thead>
        <tr>
            <th>
                ID
            </th>
            <th>
                name
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.roleId</td>
                <td>@item.roleName</td>
            </tr>
        }
    </tbody>
</table>


